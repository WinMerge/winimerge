/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

IDC_WINIMERGE MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New\tCtrl+N",                ID_FILE_NEW
        MENUITEM "New (3 panes)",               ID_FILE_NEW3
        MENUITEM "&Open Images...\tCtrl+O",     ID_FILE_OPEN
        MENUITEM "Open &3 Images...\tCtrl+3",   ID_FILE_OPEN3
        MENUITEM SEPARATOR
        MENUITEM "&Save\tCtrl+S",               ID_FILE_SAVE
        POPUP "Save &As"
        BEGIN
            MENUITEM "Save &Left As...",            ID_FILE_SAVE_LEFT_AS
            MENUITEM "Save &Middle As...",          ID_FILE_SAVE_MIDDLE_AS
            MENUITEM "Save &Right As...",           ID_FILE_SAVE_RIGHT_AS
        END
        MENUITEM "&Generate Report...",         ID_FILE_GENERATE_REPORT
        MENUITEM SEPARATOR
        MENUITEM "Re&load\tCtrl+F5",            ID_FILE_RELOAD
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+F4",               ID_FILE_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM "&Redo\tCtrl+Y",               ID_EDIT_REDO
        MENUITEM "&Cut\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM "Select &All\tCtrl+A",         ID_EDIT_SELECT_ALL
    END
    POPUP "&View"
    BEGIN
        MENUITEM "View &Differences",           ID_VIEW_VIEWDIFFERENCES
        POPUP "Diff &Block Size"
        BEGIN
            MENUITEM "&1",                          ID_VIEW_DIFFBLOCKSIZE_1
            MENUITEM "&2",                          ID_VIEW_DIFFBLOCKSIZE_2
            MENUITEM "&4",                          ID_VIEW_DIFFBLOCKSIZE_4
            MENUITEM "&8",                          ID_VIEW_DIFFBLOCKSIZE_8
            MENUITEM "1&6",                         ID_VIEW_DIFFBLOCKSIZE_16
            MENUITEM "&32",                         ID_VIEW_DIFFBLOCKSIZE_32
        END
        POPUP "&Ignore Color Difference (Color Distance Threshold)"
        BEGIN
            MENUITEM "&0",                          ID_VIEW_THRESHOLD_0
            MENUITEM "&2",                          ID_VIEW_THRESHOLD_2
            MENUITEM "&4",                          ID_VIEW_THRESHOLD_4
            MENUITEM "&8",                          ID_VIEW_THRESHOLD_8
            MENUITEM "1&6",                         ID_VIEW_THRESHOLD_16
            MENUITEM "&32",                         ID_VIEW_THRESHOLD_32
            MENUITEM "64",                          ID_VIEW_THRESHOLD_64
        END
        POPUP "Ins&ertion/Deletion Detection"
        BEGIN
            MENUITEM "&None",                       ID_VIEW_INSERTIONDELETIONDETECTION_NONE
            MENUITEM "&Vertical",                   ID_VIEW_INSERTIONDELETIONDETECTION_VERTICAL
            MENUITEM "&Horizontal",                 ID_VIEW_INSERTIONDELETIONDETECTION_HORIZONTAL
        END
        MENUITEM SEPARATOR
        MENUITEM "&Prev Page",                  ID_VIEW_PAGE_PREVPAGE
        MENUITEM "&Next Page",                  ID_VIEW_PAGE_NEXTPAGE
        POPUP "&Active Pane"
        BEGIN
            MENUITEM "&Previous Page",              ID_POPUP_CURPANE_PREVIOUSPAGE
            MENUITEM "&Next Page",                  ID_POPUP_CURPANE_NEXTPAGE
        END
        MENUITEM SEPARATOR
        POPUP "&Zoom"
        BEGIN
            MENUITEM "&400%",                       ID_VIEW_ZOOM_400
            MENUITEM "&200%",                       ID_VIEW_ZOOM_200
            MENUITEM "&100%",                       ID_VIEW_ZOOM_100
            MENUITEM "&50%",                        ID_VIEW_ZOOM_50
            MENUITEM SEPARATOR
            MENUITEM "Zoom &In\tCtrl++",            ID_VIEW_ZOOMIN
            MENUITEM "Zoom &Out\tCtrl+-",           ID_VIEW_ZOOMOUT
        END
        POPUP "&Overlay Mode"
        BEGIN
            MENUITEM "&None",                       ID_VIEW_OVERLAY_NONE
            MENUITEM "&XOR",                        ID_VIEW_OVERLAY_XOR
            MENUITEM "&Alpha Blend",                ID_VIEW_OVERLAY_ALPHABLEND
            MENUITEM "Alpha &Blend Animation",      ID_VIEW_OVERLAY_ALPHABLEND_ANIM
        END
        POPUP "Dra&gging Mode"
        BEGIN
            MENUITEM "&None",                       ID_VIEW_DRAGGINGMODE_NONE
            MENUITEM "&Move",                       ID_VIEW_DRAGGINGMODE_MOVE
            MENUITEM "&Adjust Offset",              ID_VIEW_DRAGGINGMODE_ADJUST_OFFSET
            MENUITEM "&Vertical Wipe",              ID_VIEW_DRAGGINGMODE_VERTICAL_WIPE
            MENUITEM "&Horizontal Wipe",            ID_VIEW_DRAGGINGMODE_HORIZONTAL_WIPE
            MENUITEM "&Rectangle Select",           ID_VIEW_DRAGGINGMODE_RECTANGLE_SELECT
        END
        MENUITEM "Split &Horizontally",         ID_VIEW_SPLITHORIZONTALLY
        MENUITEM "&Set Background Color",       ID_VIEW_USEBACKCOLOR
        POPUP "&Vector Image Scaling"
        BEGIN
            MENUITEM "&400%",                       ID_VIEW_VECTORIMAGESCALING_400
            MENUITEM "&200%",                       ID_VIEW_VECTORIMAGESCALING_200
            MENUITEM "&100%",                       ID_VIEW_VECTORIMAGESCALING_100
            MENUITEM "50%",                         ID_VIEW_VECTORIMAGESCALING_50
            MENUITEM "25%",                         ID_VIEW_VECTORIMAGESCALING_25
        END
        MENUITEM SEPARATOR
        MENUITEM "Extract &Text",               ID_VIEW_EXTRACTTEXT
        MENUITEM "Show &Metadata",              ID_VIEW_SHOWMETADATA
    END
    POPUP "&Merge"
    BEGIN
        MENUITEM "&Next Difference\tAlt+Down",  ID_MERGE_NEXTDIFFERENCE
        MENUITEM "&Previous Difference\tAlt+Up", ID_MERGE_PREVIOUSDIFFERENCE
        MENUITEM SEPARATOR
        MENUITEM "&First Difference\tAlt+Home", ID_MERGE_FIRSTDIFFERENCE
        MENUITEM "&Last Difference\tAlt+End",   ID_MERGE_LASTDIFFERENCE
        MENUITEM SEPARATOR
        MENUITEM "Next Conflict\tAlt+Shift+Down", ID_MERGE_NEXTCONFLICT
        MENUITEM "Previous Conflict\tAlt+Shift+Up", ID_MERGE_PREVIOUSCONFLICT
        MENUITEM SEPARATOR
        MENUITEM "Copy to &Right\tAlt+Right",   ID_MERGE_COPYTORIGHT
        MENUITEM "Copy to L&eft\tAlt+Left",     ID_MERGE_COPYTOLEFT
        MENUITEM SEPARATOR
        MENUITEM "Copy from Left\tAlt+Shift+Right", ID_MERGE_COPYFROMLEFT
        MENUITEM "Copy from Right\tAlt+Shift+Left", ID_MERGE_COPYFROMRIGHT
        MENUITEM SEPARATOR
        MENUITEM "Copy &All to Right",          ID_MERGE_COPYALLTORIGHT
        MENUITEM "Cop&y All to Left",           ID_MERGE_COPYALLTOLEFT
        MENUITEM SEPARATOR
        MENUITEM "Auto Merge\tAlt+Ctrl+M",      ID_MERGE_AUTOMERGE
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About",                      ID_HELP_ABOUT
    END
END

IDR_POPUPMENU MENU
BEGIN
    POPUP "popup"
    BEGIN
        MENUITEM "Current Pane: &Previous Page", ID_POPUP_CURPANE_PREVIOUSPAGE
        MENUITEM "Current Pane: &Next Page",    ID_POPUP_CURPANE_NEXTPAGE
    END
END


